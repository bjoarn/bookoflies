<h1>New place</h1>

<% form_for(@place) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :street_address %><br />
    <%= f.text_field :street_address %>
  </p>
  <p>
    <%= f.label :extended_address %><br />
    <%= f.text_field :extended_address %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </p>
  <p>
    <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </p>
  <p>
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :web_address %><br />
    <%= f.text_field :web_address %>
  </p>
  <p>
    <%= f.label :mixer %><br />
    <%= f.text_field :mixer %>
  </p>
  <p>
    <%= f.label :pa_system %><br />
    <%= f.text_field :pa_system %>
  </p>
  <p>
    <%= f.label :monitor_system %><br />
    <%= f.text_field :monitor_system %>
  </p>
  <p>
    <%= f.label :monitor_mixer %><br />
    <%= f.text_field :monitor_mixer %>
  </p>
  <p>
    <%= f.label :outboard %><br />
    <%= f.text_area :outboard %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', places_path %>
